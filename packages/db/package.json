{
  "name": "@blueth/db",
  "version": "0.1.0",
  "private": true,
  "description": "Blueth City database migrations and query helpers",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "migrate": "node dist/migrate.js",
    "migrate:create": "node dist/create-migration.js",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "dotenv": "^16.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.3"
  }
}
